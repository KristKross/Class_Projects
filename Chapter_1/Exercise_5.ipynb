        "from math import pi\n",
        "\n",
        "radius = int(input(\"Enter radius: \"))\n",
        "area = radius * pi ** 2\n",
        "area_rounded = \"{:.2f}\".format(area)\n",
        "print(\"The area of a circle with a radius of \" + str(radius) + \\\n",
        "      \" is \" + str(area_rounded) + \"cm^2\")"
      ],
